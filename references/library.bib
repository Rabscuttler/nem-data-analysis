keywords = {AEMO,BETTA,CAISO,CNE,Electricity market,Energy industry,Energy supply systems,NEM,NETA,Nord Pool,SING},
number = {2},
pages = {169--200},
title = {{Electricity markets: An overview and comparative study}},
