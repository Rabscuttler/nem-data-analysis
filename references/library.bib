@book{MacGill2013,
abstract = {The Australian National Electricity Market is widely considered a successful example of electricity industry restructuring. It has delivered reliable electricity supply with apparent economic efficiency gains over the former monopoly utility arrangements, and in the context of significant industry challenges including peak demand growth and a changing generation mix. However, to date it has largely failed in reducing greenhouse emissions although this was an explicit target of public policy when restructuring initially commenced. This chapter explores the reasons for this divergent performance and possible options to address it. While commonly ascribed to a lack of political will, restructuring itself has also created difficulties for effective climate action through a narrow economic focus that resulted in: the creation of large politically powerful market incumbents opposed in many cases to improved environmental outcomes, retail market arrangements encouraging competition in energy sales rather than in efficient provision of desired energy services, and poor governance of external climate policies. There are also more fundamental challenges with the current market framework that will need to be addressed for low-carbon industry transformation. {\textcopyright} 2013 Elsevier Inc. All rights reserved.},
author = {MacGill, Iain and Healy, Stephen},
booktitle = {Evolution of Global Electricity Markets: New Paradigms, New Challenges, New Approaches},
doi = {10.1016/B978-0-12-397891-2.00020-1},
file = {:home/abi/version{\_}control/Research/Markets/Lessons from Australian Restructuring and Climate Policy.pdf:pdf},
isbn = {9780123978912},
keywords = {Australian National Electricity Market,Climate change,Environmental regulation,Micro-economic reform},
pages = {615--644},
publisher = {Elsevier Inc.},
title = {{Is Electricity Industry Reform the Right Answer to the Wrong Question? Lessons from Australian Restructuring and Climate Policy}},
